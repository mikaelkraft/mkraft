{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/((?!api|ads\\.txt|sw\\.js|favicon\\.ico).*)",
      "destination": "/"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs24.x"
    }
  }
}
